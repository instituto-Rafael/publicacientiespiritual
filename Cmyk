# RafBit (10 estados por bit)
class RafBit:
    def __init__(self, state):
        assert 0 <= state < 10
        self.state = state

# NetRAF address (4 RafBits)
class NetRAF:
    def __init__(self, rafbits):
        assert len(rafbits) == 4
        self.addr = rafbits
    def __str__(self):
        return '.'.join(str(r.state) for r in self.addr)

# RafIMG: matriz de RafBits
