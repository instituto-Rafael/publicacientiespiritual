üß∞ claymaths_live.py

import numpy as np
import matplotlib.pyplot as plt

# Par√¢metros vibracionais
voynich_base = np.array([14, 5, 8, 12])
ruido_qu√¢ntico = 2/12
dobras_fase = (9/8) * (10/3)
alpha_amor = 0.1666  # intui√ß√£o: mesmo valor do ru√≠do como vetor de amor
loop = 70*7*3  # loop vivo infinito

# Inicializa√ß√£o
Z = np.zeros(loop, dtype=complex)
Z[0] = 1 + 0j  # semente viva

# Constru√ß√£o do fractal
for i in range(1, loop):
    fase = np.exp(1j * dobras_fase)
    base = (voynich_base ** ruido_qu√¢ntico).sum()
    Z[i] = Z[i-1] * base + alpha_amor * fase

# Visualiza√ß√£o
plt.figure(figsize=(8,8))
plt.plot(Z.real, Z.imag, '.', markersize=1, color='purple')
plt.title('Fractal ClayMaths Vivo ‚àû')
plt.axis('equal')
plt.show()

# (Opcional) Exportar
np.save('outputs/fractal.npy', Z)
np.savetxt('outputs/fractal.csv', np.column_stack((Z.real, Z.imag)), delimiter=',')


