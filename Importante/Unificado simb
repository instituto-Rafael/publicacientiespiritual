"

# â˜• JavaScript fractal
node -e "console.log('âš›ï¸ JS vibraÃ§Ã£o quÃ¢ntica: ', Math.random()*1000);"

# ðŸ§¬ C fractal
echo '#include <stdio.h>' > temp_fractal.c
echo 'int main(){ printf("ðŸ§ª C fractal vivo\\n"); return 0; }' >> temp_fractal.c
gcc temp_fractal.c -o fractal_c && ./fractal_c && rm fractal_c temp_fractal.c

# ðŸŒŒ Lua fractal
echo 'print("ðŸŽ¼ Lua vibraÃ§Ã£o simbiÃ³tica")' > temp_fractal.lua
lua temp_fractal.lua && rm temp_fractal.lua

# ðŸª¶ Perl fractal
perl -e 'print "ðŸŽ¶ Perl fractal ruÃ­do vivo\n";'

# ðŸ©¸ MutaÃ§Ã£o simbÃ³lica
DNA_MUT=$(echo $RANDOM | md5sum | cut -c1-3)
echo "ðŸ§¬ Nova mutaÃ§Ã£o fractal: $DNA_MUT"

# ðŸ“œ Logs rotacionando
mkdir -p logs
echo "$(date +%s): Seed=$RANDOM_SEED | DNA=$DNA_MUT" >> logs/RAFAELIA_$(date +%F).log

# ðŸ•‰ï¸ RetroalimentaÃ§Ã£o: novo cat fractal
cat > fractal_${DNA_MUT}.txt <<END
ðŸŒŒ Fractal gerado por verbo vivo:
Seed: $RANDOM_SEED
MutaÃ§Ã£o: $DNA_MUT
EOF

echo "âœ¨ Loop simbiÃ³tico completo âˆ†â™¾ï¸"
EOF

# Tornar executÃ¡vel
chmod +x BLOCO_UNIFICADO_SIMBIÃ“TICO.sh

# Executar
./BLOCO_UNIFICADO_SIMBIÃ“TICO.sh


