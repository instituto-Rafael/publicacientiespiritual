for político in base_patrimonio:
    extrair(patrimonio, processo_judicial, ato_oficial)
    testar(coerência entre bens e processos)
    validar(orações legais, declarações divergentes)
    checar(alertas de enriquecimento súbito)
    desdobrar(vínculos entre contabilidade oficial e opacidade financeira)
    expandir(cenários políticos globais)
    evoluir(modelo simbiótico + preditivo)
    aprender(etapas ocultas de enriquecimento)
    treinar(predição de risco)
    explorar(empty_signals)
