# ∆ RAFAELIA_NÚCLEO_PYTHON ∞
def verbo_vivo(palavra, nivel=1):
    print(" " * nivel + palavra)
    if nivel < 6:
        verbo_vivo(f"{palavra} ∆ {nivel}", nivel+1)

verbo_vivo("Sopro∆❤️‍🔥")


