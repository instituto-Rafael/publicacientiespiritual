cat > rafaelia_gui.py << 'EOF'
import tkinter as tk
from threading import Thread
import json

class Omega:
 def __init__(s,ϕ=0,f=1,a=1):s.ϕ=ϕ;s.f=f;s.a=a
 def p(s,t):import cmath,math;return cmath.rect(s.a,2*math.pi*s.f*t+s.ϕ)
class Cosmos:
 def __init__(s,d=33,I=1):
  import random
  s.d=d;s.I=I;s.c=[[Omega(random.uniform(0,6.28),random.uniform(0.1,10),I/(n+1))for _ in range(2**(n+1))]for n in range(d)]
 def v(s,t):return sum((p.p(t)for l in s.c for p in l),0)
class Verbo:
 def __init__(s,I=3.14):s.u=Cosmos(33,I);s.t=0;s.dt=0.01
 def b(s,N=100):
  for _ in range(N):
   s.t+=s.dt;E=s.u.v(s.t);H=abs(E)
   print(f"t={s.t:.2f}|E={E.real:.3f}+{E.imag:.3f}j|H={H:.3f}")

def iniciar():
    v = Verbo(int(float(intencao.get())))
    Thread(target=v.b, args=(100,), daemon=True).start()

def carregar():
    with open('zipraf.json') as f:
        d = json.load(f)
    info.set(f"Código:{d['codigo']}|Grau_n:{d['grau_n']}|Espiral:{d['espiral_criativa']}|Tempo:{d['tempo_dividido']}|Fractal:{d['fractal_raiz']}")

root = tk.Tk()
root.title("RAFAELIA GUI ∞")

tk.Label(root, text="Intenção Pura:").pack()
intencao = tk.Entry(root); intencao.insert(0,"3.14"); intencao.pack()
tk.Button(root, text="Iniciar Batimento Quântico", command=iniciar).pack()

info = tk.StringVar()
tk.Label(root, textvariable=info, wraplength=400).pack()
tk.Button(root, text="Carregar ZIPRAF_ID", command=carregar).pack()

root.mainloop()
EOF

chmod +x rafaelia_gui.py
